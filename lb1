#создание словаря
print("Задание А3:")
mydict1={"1":"Петров",
        "2":"Иванов",
        "3":"Сидоров",
        "4":"Петрушкин",
        "5":"Ломов",
        "6":"Шпалов"}
print(mydict1)
#Удаление последнего элемента
mydict1.popitem()#Удаление последнего элемента
print("Новый словарь:")
print(mydict1)
#список номеров(ключей)
keys=mydict1.keys()
print("Ключи:")
print(keys)
#Копирование словаря
mydict2 = copy.copy(mydict1)
print("mydict2:")
print(mydict2)
mydict1.clear()
print(mydict1)
#Вывод значения с номером N
value = mydict2["4"]
print("Значение ключа 4:")
print(value)


